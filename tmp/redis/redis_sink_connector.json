{
    "name": "redis-sink-accounts-connector",
    "config": {
        "connector.class": "com.github.jcustenborder.kafka.connect.redis.RedisSinkConnector",
        "tasks.max": "1",
        "topics": "probation.raw.accounts",
        "redis.hosts": "db-redis",
        "redis.port": "6379",
        "redis.value.type": "json",
        "redis.key.prefix": "probation",
        "redis.key.field": "account:counter",
        "key.converter": "org.apache.kafka.connect.storage.ByteArrayConverter",
        "value.converter": "org.apache.kafka.connect.storage.ByteArrayConverter"
    }
}